<div class="hor_slider">
    <script>
        $(document).ready(function () {
            sliderposition = 0;
            slides = [
                {
                    image: "url('/Content/Img/mainslide1.jpg')",
                    h1: "Отопление",
                    container: "Мы оказываем весь спектр услуг по монтажу отопительных систем",
                    color: "rgba(202,72,66,0.7)"
                },
                {
                    image: "url('/Content/Img/mainslide2.jpg')",
                    h1: "Водоснабжение",
                    container: "Монтаж систем водоснабжения под ключ",
                    color: "rgba(66,139,202,0.7)"
                },
                {
                    image: "url('/Content/Img/mainslide3.jpg')",
                    h1: "Канализация",
                    container: "Подключение автономной канализации",
                    color: "rgba(66,202,72,0.7)"
                }
            ];
            setInterval(function () {
                sliderposition++;
                if (sliderposition > slides.length - 1) {
                    sliderposition = 0;
                }
                var full_page_photo = $(".full_page_photo");
                full_page_photo.css("background-image", slides[sliderposition].image);
                full_page_photo.find("h1").text(slides[sliderposition].h1);
                full_page_photo.find(".skincolored").css("background-color", slides[sliderposition].color);
                full_page_photo.find(".container p").text(slides[sliderposition].container);
            }, 7000);
        });
    </script>
