				<script>
					$(document).ready(function(){
						
						$(".container2").hover(function(){$(this).css("opacity","1")}, function(){$(this).css("opacity","0.7")});
						
						var MainMenu = $(".container:not(.container2)");
						var AdditionalMenu = $(".container2");
						window.onscroll = function() {
							if (window.pageYOffset > MainMenu.height()){
								MainMenu.hide();
								AdditionalMenu.show();
							}
